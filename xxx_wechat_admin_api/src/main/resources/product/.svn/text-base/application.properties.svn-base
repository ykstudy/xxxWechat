mybatis.typeAliasesPackage=com.qiaohu.wechat.core.entity
#DataSource连接池
datasource.type=com.alibaba.druid.pool.DruidDataSource
#DataSource连接配置
datasource.write.url = jdbc:mysql://172.16.0.18:3306/fwh?characterEncoding=utf8
datasource.write.username= fwh
datasource.write.password= AdkTvV7*
datasource.write.driver-class-name= com.mysql.jdbc.Driver
datasource.write.filters= stat
datasource.write.maxActive= 20
